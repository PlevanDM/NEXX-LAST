  "usb_c_diagnostics": {
    "title": "USB-C диагностика для iPhone 15/16",
    "controller": "CD3217B12 / SN2012017 / SN2012027",
    "diagnostic_steps": [
      {
        "step": 1,
        "check": "Напряжение VBUS",
        "expected": "5V минимум, до 20V при USB PD",
        "tool": "USB-C тестер или мультиметр"
      },
      {
        "step": 2,
        "check": "CC линии",
        "expected": "1.6V-3.3V в зависимости от режима",
        "note": "Неправильное напряжение CC = проблема с USB PD negotiation"
      },
      {
        "step": 3,
        "check": "Data линии (D+/D-)",
        "expected": "Переключение между режимами",
        "note": "Отсутствие данных при зарядке = возможно ROM pairing issue"
      },
      {
        "step": 4,
        "check": "ROM чип (8Nxxxx)",
        "note": "CD3217 требует paired ROM чип. При замене CD3217 необходимо также перенести ROM с донора"
      }
    ],
    "common_issues": [
      {
        "symptom": "Застревает на 5V зарядке",
        "cause": "CD3217 failure или ROM corruption",
        "solution": "Замена CD3217 + ROM с одного донора"
      },
      {
        "symptom": "Нет данных через USB-C",
        "cause": "USB контроллер или connector damage",
        "solution": "Проверить порт, затем диагностика CD3217"
      },
      {
        "symptom": "Baseband failure после падения",
        "cause": "CD3217 связан с baseband на iPhone 15/16",
        "solution": "Диагностика baseband PMIC, проверка CD3217"
      }
    ]
  },
