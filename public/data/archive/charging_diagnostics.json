  "diagnostic_steps": [
    {
      "step": 1,
      "title": "Проверка 5V перед Charging IC",
      "description": "Измерить 5V на конденсаторе перед charging IC",
      "expected": "5V",
      "if_no_voltage": "Проблема с USB входом / контроллером",
      "if_voltage_present": "Перейти к шагу 2"
    },
    {
      "step": 2,
      "title": "Понимание логики USB верификации",
      "description": "Charging IC НЕ знает, хороший ли кабель. Валидация кабеля выполняется USB контроллером (Tristar/Hydra/Kraken)",
      "note": "Если USB IC не может верифицировать кабель - устройство не будет заряжаться по кабелю, но беспроводная зарядка может работать"
    },
    {
      "step": 3,
      "title": "Проверка линии USB верификации",
      "description": "Если 5V отсутствует на USB контроллере, проверить PP_VAR_USB_RVP_TIGRIS_R (также PP_VAR_USB_RVP)",
      "note": "Эта линия необходима для валидации USB кабеля"
    },
    {
      "step": 4,
      "title": "Проверка RVP резистора 10Ω",
      "applicable_to": [
        "iPhone 8",
        "iPhone X",
        "iPhone 13",
        "iPhone 14"
      ],
      "procedure": [
        "Найти резистор 10Ω на линии VAR_USB_RVP",
        "Измерить напряжение на обеих pad",
        "Обе стороны должны показывать ~5V"
      ],
      "interpretation": {
        "one_side_only": "Открытый резистор или проблема downstream",
        "no_voltage_both": "Проблема с USB IC upstream"
      }
    },
    {
      "step": 5,
      "title": "Проверка PWR_GATE_EN_VBUS_1_VALID",
      "description": "Измерить напряжение в тестовой точке PWR_GATE_EN_VBUS_1_VALID",
      "expected": "3.7V - 5V (любое напряжение выше 0V)",
      "interpretation": {
        "voltage_present": "USB IC работает",
        "zero_voltage": "USB IC неисправен или не запитан"
      }
    },
    {
      "step": 6,
      "title": "Проверка беспроводной зарядки",
      "description": "Беспроводная зарядка НЕ требует USB верификации",
      "note": "Если беспроводная зарядка работает, а кабельная нет - вероятно проблема с USB контроллером или RVP линией"
    },
    {
      "step": 7,
      "title": "Charging IC Power Conversion",
      "description": "Charging IC преобразует 5V → ~3.7V для батареи",
      "note": "Если 5V присутствует, но батарея не заряжается - подозревать charging IC или его выходной каскад"
    },
    {
      "step": 8,
      "title": "Проверка CHG_BOOT линии (Diode Mode)",
      "description": "Найти конденсатор на CHG_BOOT, измерить в режиме диода",
      "expected_readings": {
        "from_tigris_side": "~9V",
        "from_chg_lx_side": "~4V"
      },
      "fault_indication": "Одинаковые показания с обеих сторон = неисправность"
    }
  ],
